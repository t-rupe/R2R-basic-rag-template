{
  "embedding": {
    "provider": "openai",
    "model": "text-embedding-3-large",
    "dimension": 4096,
    "batch_size": 16
  },
  "evals": {
    "provider": "deepeval",
    "frequency": 1.0
  },
  "ingestion":{
    "provider": "local",
    "text_splitter": {
      "type": "recursive_character",
      "chunk_size": 512,
      "chunk_overlap": 20
    }
  },
  "language_model": {
    "provider": "litellm"
  },
  "logging_database": {
    "provider": "sciphi"
  },
  "vector_database": {
    "provider": "sciphi"
  },
  "app": {
    "max_logs": 100
  }  
}
